<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-xs-12 element-center">
        <h2><b>Elecciones para presidente municipal</b> | 2015 </h2>
    </div>
</div>

<div class="ibox" ng-controller="electionGlobalCtrl">
    <div class="ibox-title">
        <h5>
            PREP elecciones para presidente municipal
            <br />
            <br />
            <select class="form-control" ng-options="item as item.value for item in lstTypes" ng-model="type"></select>
        </h5>
        <div class="ibox-tools">
            <span class="label label-success"><h4>Distritos</h4></span>
        </div>
    </div>
    <div class="ibox-content">
